{
  "expo": {
    "name": "rent-car",
    "slug": "rent-car",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "c99031e6-6fda-4a2d-a06b-4bc257e1fa95"
      }
    },
    "android": {
      "package": "com.ridho21.rentcar",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "owner": "ridho21"
  }
}
